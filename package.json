{
  "name": "create-akalli-app",
  "description": "A starter kit for creating Akalli apps",
  "bin": "./bin/akalli-cli.js",
  "version": "0.1.1",
  "main": "node_modules/expo/AppEntry.js",
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "npm run lint",
      "npm run test"
    ]
  },
  "scripts": {
    "start": "expo start --clear --dev-client",
    "expo:go": "expo start --clear",
    "android": "expo start --android",
    "commit": "git-cz",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "npx husky --install",
    "prepare": "husky install",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "clear": "rm -rf node_modules && npm install",
    "test:watch": "jest --watch --coverage=false --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "test": "jest --passWithNoTests",
    "snapshots": "jest -u --coverage=false",
    "build:dev": "eas build --profile development",
    "buid:android": "eas build --platform android",
    "manage:expo": "expo credentials:manager",
    "build:internal": "eas build --profile preview --platform android"
  },
  "jest": {
    "preset": "jest-expo",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-icons)"
    ],
    "setupFiles": [
      "./jestSetupFile.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@akalli/components": "^0.2.2",
    "@akalli/icons": "^0.0.7",
    "@akalli/navigation": "^0.1.3",
    "@akalli/state": "^0.0.3",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.4.1",
    "@react-navigation/stack": "^6.1.1",
    "@reduxjs/toolkit": "^1.8.3",
    "expo": "~45.0.0",
    "expo-dev-client": "~1.0.1",
    "expo-status-bar": "~1.3.0",
    "husky": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.34.0",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.18.10",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@testing-library/jest-native": "^4.0.8",
    "@testing-library/react-native": "^11.0.0",
    "@types/jest": "^27.4.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-redux": "^7.1.22",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^28.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "jest-expo": "^45.0.1",
    "prettier": "2.5.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-flipper": "^2.0.2",
    "typescript": "~4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": "dsilva@akalli.com.br"
}

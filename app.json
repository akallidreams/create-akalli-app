{
  "expo": {
    "name": "akalli-app",
    "slug": "akalli-app",
    "owner": "akalli",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "androidStatusBar": {
      "backgroundColor": "#6A59AA",
      "translucent": true
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A59AA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "light",
      "versionCode": 1,
      "permissions": [],
      "package": "akalli.App"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "flipperHack": "React native package is running"
    }
  }
}
